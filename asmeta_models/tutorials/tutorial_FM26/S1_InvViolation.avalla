scenario S1_InvairantViolation
load SchedulerInv.asm

set fin(JOB1) := false;
set fin(JOB2) := false;
set fin(JOB3) := false;

// jobs are ready
check st(JOB1) = RDY and st(JOB2) = RDY and st(JOB3) = RDY;

step
// one is running
check (st(JOB1) = RUN or st(JOB2) = RUN or st(JOB3) = RUN);
step

// finish all the jobs
set fin(JOB1) := true;
set fin(JOB2) := true;
set fin(JOB3) := true;
step
