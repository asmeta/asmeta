scenario S4_PickDeterministic
load Scheduler.asm

set fin(JOB1) := false;
set fin(JOB2) := false;
set fin(JOB3) := false;

check not idle;
check st(JOB1) = RDY and st(JOB2) = RDY and st(JOB3) = RDY;

pick $j1 := JOB3;

step

check st(JOB3) = RUN;
check st(JOB1) = RDY and st(JOB2) = RDY;

set fin(JOB3) := true;

pick $j1 := JOB2;

step

check st(JOB3) = FIN;
check st(JOB2) = RUN;
check st(JOB1) = RDY;