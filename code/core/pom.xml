<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <artifactId>asmeta.core</artifactId>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.11.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <packaging>pom</packaging>
  <modules>
  	<module>asmeta.emf</module>
  	<module>asmeta.parser/asmeta.parser</module>
  	<module>asmeta.parser/asmeta.parser.cli</module>
  	<module>asmeta.parser/asmeta.parser.test</module>
  	<module>asmeta.simulator/asmeta.simulator</module>
  	<module>asmeta.simulator/asmeta.simulator.cli</module>
  	<module>asmeta.simulator/asmeta.simulator.test</module>
  	<module>asmeta.cli</module>
  </modules>
  <parent>
  	<groupId>asmeta</groupId>
  	<artifactId>asmeta.all</artifactId>
  	<version>0.0.1-SNAPSHOT</version>
  	<relativePath>../..</relativePath>
  </parent>
</project>